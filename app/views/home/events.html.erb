	<!-- Carousel -->
	<div class="row nopadding">
		<div class="embed-responsive embed-responsive-4by6 short">
		  	<iframe width="100%" height="500" src="https://www.youtube.com/embed/QjKKQgtxqKg" frameborder="0" allowfullscreen></iframe>
		</div>	
		<!-- About The Watershed Council -->
		<h1 class="text-center thicker bigmgn">Watershed Events</h1>
		<h5 class=" text-center text-muted">Our field trips & meetings are free and open to the public. </br>
			We encourage you to join us to learn more about the Puyallup River Watershed.
		</h5>
		<div class="center-icon center-block">
			<div class="img-square">
				<%= image_tag("break-element.jpg", class:"img-responsive") %>
			</div>
		</div>
		<div class="col-md-12">
			<% @events.each do |e| %> 
			<div class="col-sm-12 col-md-8 col-md-offset-2 event">
				<% if e.link? %>
					<h2><a href="<%= e.link %>" class="ebutton pull-right" target="blank"><i class="fa fa-plus"></i> Google Cal</a></h2>
				<% else %>
				<% end %>
				<h2><%= e.title %></h2>
				<h4><%= e.date.strftime("%b. %d, %Y @ %l:%M %p") %></h4>
				<div class="border description">
					<p class="white"><%= simple_format(e.description) %></p>
				</div>
			</div>
			<% end %>
		</div>
	</div>