<p id="notice"><%= notice %></p>

<h1>Listing Minutes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @minutes.each do |minute| %>
      <tr>
        <td><%= minute.title %></td>
        <td><%= minute.location %></td>
        <td><%= minute.street %></td>
        <td><%= minute.city %></td>
        <td><%= minute.state %></td>
        <td><%= minute.zip %></td>
        <td><%= minute.date %></td>
        <td><%= link_to 'Show', minute %></td>
        <td><%= link_to 'Edit', edit_minute_path(minute) %></td>
        <td><%= link_to 'Destroy', minute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Minute', new_minute_path %>
